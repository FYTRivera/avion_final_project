<div id="<%= dom_id meeting %>">
  <p>
    <strong>Name:</strong>
    <%= meeting.name %>
  </p>

  <p>
    <strong>Doctor:</strong>
    <%= @doctor.first_name %> <%= @doctor.last_name %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= meeting.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= meeting.end_time %>
  </p>

  <p>
    <strong>Is Booked? :</strong>
    <%= meeting.is_booked %> 
  </p>

  <p>
    <strong>Client:</strong>
    <%= meeting.client_email %> 
  </p>

</div>
